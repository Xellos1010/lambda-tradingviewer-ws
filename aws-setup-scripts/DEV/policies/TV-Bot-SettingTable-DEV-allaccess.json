{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "ManageSettingTable",
            "Effect": "Allow",
            "Action": [
                "dynamodb:GetItem",
                "dynamodb:PutItem",
                "dynamodb:UpdateItem",
                "dynamodb:DeleteItem",
                "dynamodb:Query",
                "dynamodb:Scan"
            ],
            "Resource": "arn:aws:dynamodb:us-east-1:697426589657:table/TV-Bot-SettingTable-DEV"
        },
        {
            "Sid": "ManageSettingTableIndexes",
            "Effect": "Allow",
            "Action": [
                "dynamodb:Query"
            ],
            "Resource": "arn:aws:dynamodb:us-east-1:697426589657:table/TV-Bot-SettingTable-DEV/index/*"
        }
    ]
}
